{
  "extends": "eslint-config-egg/typescript",
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "quotes": ["error", "single"],
    "semi": ["error", "never"],
    "array-bracket-spacing": ["error", "never"],
    "@typescript-eslint/semi": ["error", "never"],
    "comma-dangle": ["error", "never"]
  }
}
